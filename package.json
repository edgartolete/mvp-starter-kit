{
  "name": "mvp-starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "cl:preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cl:deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cl:cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "auth:generate": "pnpm dlx @better-auth/cli@latest generate --config ./better-auth.config.ts --output ./src/db/schema/auth.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "npx tsx src/db/seed/index.ts"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/material-nextjs": "^7.3.6",
    "@neondatabase/serverless": "^1.0.2",
    "@opennextjs/cloudflare": "^1.14.6",
    "@tanstack/react-query": "^5.90.12",
    "better-auth": "^1.4.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.0",
    "hono": "^4.11.0",
    "jotai": "^2.15.2",
    "jotai-immer": "^0.4.1",
    "next": "16.0.7",
    "nuqs": "^2.8.2",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "jotai-devtools": "^0.13.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "wrangler": "^4.54.0"
  },
  "packageManager": "pnpm@9.1.1+sha1.09ada6cd05003e0ced25fb716f9fda4063ec2e3b"
}
